{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "BetterML",
  "version": "1.0.8",
  "manifest_version": 3,
  "description": "Making Manyland Better.",
  "homepage_url": "https://github.com/ZoltarML/",
  "content_scripts": [
    {
      "matches": [
        "https://saltfree.antisa.lt/*",
        "http://saltfree.antisa.lt/*"
      ],
      "js": [
        "inject.js"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "mods/loader.js",
        "mods/extrasounds.js",
        "mods/performance.js",
        "mods/exporter.js",
        "mods/rankhand.js",
        "mods/friendsplus.js",
        "mods/movepainter.js",
        "mods/freecam.js",
        "mods/norefresh.js",
        "mods/importer.js",
        "mods/pixelCopyImage.js",
        "mods/speechtotext.js",
        "mods/highcontrast.js",
        "mods/debugmode.js",
        "mods/buildplus.js",
        "mods/farbackconverter.js",
        "dependencies/artyom.window.min.js",
        "dependencies/deobf.js",
        "dependencies/jimp.min.js",
        "dependencies/quantize.js"
      ],
      "matches": [
        "http://saltfree.antisa.lt/*",
        "https://saltfree.antisa.lt/*"
      ]
    }
  ],
  "action": {
    "default_title": "BetterML",
    "default_popup": "index.html"
  }
}
